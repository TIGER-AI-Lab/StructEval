<recipe>
  <title>Rainbow Veggie Skewers with Peanut Sauce</title>
  <author>
    <name>Gordon Ramsay Jr.</name>
    <contact>
      <email>gordon.ramsay.jr@example.com</email>
    </contact>
  </author>
  <ingredients>
    <ingredient>
      <name>Red Bell Pepper</name>
      <quantity>1</quantity>
      <unit>piece</unit>
    </ingredient>
    <ingredient>
      <name>Zucchini</name>
      <quantity>1</quantity>
      <unit>piece</unit>
    </ingredient>
    <ingredient>
      <name>Red Onion</name>
      <quantity>1</quantity>
      <unit>piece</unit>
    </ingredient>
        <ingredient>
      <name>Cherry Tomatoes</name>
      <quantity>1</quantity>
      <unit>pint</unit>
    </ingredient>
  </ingredients>
  <instructions>
    <step>
      <order>1</order>
      <description>Chop all vegetables into 1-inch pieces.</description>
    </step>
    <step>
      <order>2</order>
      <description>Thread vegetables onto skewers, alternating colors.</description>
    </step>
        <step>
      <order>3</order>
      <description>Grill skewers for 5-7 minutes, turning occasionally, until tender-crisp.</description>
    </step>
  </instructions>
  <metadata>
    <cuisine>Vegetarian</cuisine>
    <prepTime>PT20M</prepTime>
  </metadata>
</recipe>