<recipe>
  <title>Quinoa and Avocado Salad</title>
  <author>
    <name>Alex Johnson</name>
    <contact>
      <email>alex.johnson@example.com</email>
    </contact>
  </author>
  <ingredients>
    <ingredient>
      <name>Quinoa</name>
      <quantity>1</quantity>
      <unit>cup</unit>
    </ingredient>
    <ingredient>
      <name>Avocado</name>
      <quantity>1</quantity>
      <unit>medium</unit>
    </ingredient>
    <ingredient>
      <name>Cherry Tomatoes</name>
      <quantity>1</quantity>
      <unit>cup</unit>
    </ingredient>
    <ingredient>
      <name>Lemon Juice</name>
      <quantity>2</quantity>
      <unit>tablespoons</unit>
    </ingredient>
    <ingredient>
      <name>Salt</name>
      <quantity>1</quantity>
      <unit>teaspoon</unit>
    </ingredient>
  </ingredients>
  <steps>
    <step>
      <order>1</order>
      <instruction>Rinse the quinoa under cold water.</instruction>
      <duration>
        <minutes>2</minutes>
      </duration>
    </step>
    <step>
      <order>2</order>
      <instruction>Cook the quinoa according to package instructions.</instruction>
      <duration>
        <minutes>15</minutes>
      </duration>
    </step>
    <step>
      <order>3</order>
      <instruction>Dice the avocado and cherry tomatoes.</instruction>
      <duration>
        <minutes>5</minutes>
      </duration>
    </step>
    <step>
      <order>4</order>
      <instruction>In a large bowl, combine the cooked quinoa, avocado, and cherry tomatoes.</instruction>
      <duration>
        <minutes>2</minutes>
      </duration>
    </step>
    <step>
      <order>5</order>
      <instruction>Drizzle with lemon juice and season with salt.</instruction>
      <duration>
        <minutes>1</minutes>
      </duration>
    </step>
  </steps>
  <nutrition>
    <calories>350</calories>
    <fat>
      <grams>15</grams>
    </fat>
    <protein>
      <grams>8</grams>
    </protein>
    <carbohydrates>
      <grams>45</grams>
    </carbohydrates>
  </nutrition>
  <tags>
    <tag>healthy</tag>
    <tag>vegan</tag>
  </tags>
