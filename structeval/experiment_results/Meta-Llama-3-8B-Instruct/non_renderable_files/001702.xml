
&lt;recipe&gt;
    &lt;title&gt;Delicious Chicken Fajitas&lt;/title&gt;
    &lt;author&gt;
        &lt;name&gt;John Doe&lt;/name&gt;
        &lt;contact&gt;johndoe@email.com&lt;/contact&gt;
    &lt;/author&gt;
    &lt;ingredients&gt;
        &lt;ingredient&gt;
            &lt;name&gt;Boneless, skinless chicken breasts&lt;/name&gt;
            &lt;quantity&gt;1&lt;/quantity&gt;
            &lt;unit&gt;pounds&lt;/unit&gt;
        &lt;/ingredient&gt;
        &lt;ingredient&gt;
            &lt;name&gt;Bell peppers&lt;/name&gt;
            &lt;quantity&gt;2&lt;/quantity&gt;
            &lt;unit&gt;pieces&lt;/unit&gt;
        &lt;/ingredient&gt;
        &lt;ingredient&gt;
            &lt;name&gt;Onions&lt;/name&gt;
            &lt;quantity&gt;1&lt;/quantity&gt;
            &lt;unit&gt;medium&lt;/unit&gt;
        &lt;/ingredient&gt;
        &lt;ingredient&gt;
            &lt;name&gt;Fajita seasoning&lt;/name&gt;
            &lt;quantity&gt;1&lt;/quantity&gt;
            &lt;unit&gt;packet&lt;/unit&gt;
        &lt;/ingredient&gt;
    &lt;/ingredients&gt;
    &lt;steps&gt;
        &lt;step&gt;
            &lt;description&gt;Slice the chicken breasts into thin strips and season with fajita seasoning&lt;/description&gt;
            &lt;duration_minutes&gt;10&lt;/duration_minutes&gt;
            &lt;tools&gt;slicing knife, cutting board&lt;/tools&gt;
        &lt;/step&gt;
        &lt;step&gt;
            &lt;description&gt;Heat a large skillet over medium-high heat and add the chicken and bell peppers&lt;/description&gt;
            &lt;duration_minutes&gt;15&lt;/duration_minutes&gt;
            &lt;tools&gt;large skillet, stove&lt;/tools&gt;
        &lt;/step&gt;
        &lt;step&gt;
            &lt;description&gt;Cook the chicken and bell peppers for 5-7 minutes or until the chicken is cooked through and the bell peppers are tender&lt;/description&gt;
            &lt;duration_minutes&gt;7&lt;/duration_minutes&gt;
            &lt;tools&gt;large skillet, stove&lt;/tools&gt;
        &lt;/step&gt;
    &lt;/steps&gt;
    &lt;metadata&gt;
        &lt;cuisine&gt;Mexican&lt;/cuisine&gt;
        &lt;difficulty&gt;Easy&lt;/difficulty&gt;
        &lt;servings&gt;4&lt;/servings&gt;
        &lt;prep_time&gt;PT15M&lt;/prep_time&gt;
        &lt;cook_time&gt;PT20M&lt;/cook_time&gt;
    &lt;/metadata&gt;
    &lt;nutrition&gt;
        &lt;calories&gt;400&lt;/calories&gt;
        &lt;fat&gt;20&lt;/fat&gt;
        &lt;protein&gt;30&lt;/protein&gt;
    &lt;/nutrition&gt;
&lt;/recipe&gt;
