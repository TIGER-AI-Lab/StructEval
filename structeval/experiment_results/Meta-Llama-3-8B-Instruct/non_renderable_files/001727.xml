
&lt;recipe&gt;
    &lt;name&gt;Delicious Chicken Fajitas&lt;/name&gt;
    &lt;author&gt;John Doe&lt;/author&gt;
    &lt;cuisine&gt;Mexican&lt;/cuisine&gt;
    &lt;prepTime&gt;15&lt;/prepTime&gt;
    &lt;cookTime&gt;20&lt;/cookTime&gt;
    &lt;servings&gt;4&lt;/servings&gt;
    &lt;ingredients&gt;
        &lt;ingredient&gt;
            &lt;name&gt;Chicken breast&lt;/name&gt;
            &lt;quantity&gt;1.5&lt;/quantity&gt;
            &lt;unit&gt;pounds&lt;/unit&gt;
        &lt;/ingredient&gt;
        &lt;ingredient&gt;
            &lt;name&gt;Bell peppers&lt;/name&gt;
            &lt;quantity&gt;2&lt;/quantity&gt;
            &lt;unit&gt;pieces&lt;/unit&gt;
        &lt;/ingredient&gt;
        &lt;ingredient&gt;
            &lt;name&gt;Onions&lt;/name&gt;
            &lt;quantity&gt;1&lt;/quantity&gt;
            &lt;unit&gt;medium&lt;/unit&gt;
        &lt;/ingredient&gt;
        &lt;ingredient&gt;
            &lt;name&gt;Fajita seasoning&lt;/name&gt;
            &lt;quantity&gt;1&lt;/quantity&gt;
            &lt;unit&gt;teaspoon&lt;/unit&gt;
        &lt;/ingredient&gt;
    &lt;/ingredients&gt;
    &lt;instructions&gt;
        &lt;step&gt;
            &lt;order&gt;1&lt;/order&gt;
            &lt;description&gt;Cut the chicken into thin strips and season with fajita seasoning&lt;/description&gt;
        &lt;/step&gt;
        &lt;step&gt;
            &lt;order&gt;2&lt;/order&gt;
            &lt;description&gt;Heat a large skillet over medium-high heat and add the chicken&lt;/description&gt;
        &lt;/step&gt;
        &lt;step&gt;
            &lt;order&gt;3&lt;/order&gt;
            &lt;description&gt;Add the bell peppers and onions to the skillet and cook until tender&lt;/description&gt;
        &lt;/step&gt;
    &lt;/instructions&gt;
    &lt;nutrition&gt;
        &lt;calories&gt;400&lt;/calories&gt;
        &lt;protein&gt;30&lt;/protein&gt;
        &lt;fat&gt;20&lt;/fat&gt;
        &lt;carbohydrates&gt;20&lt;/carbohydrates&gt;
    &lt;/nutrition&gt;
&lt;/recipe&gt;
