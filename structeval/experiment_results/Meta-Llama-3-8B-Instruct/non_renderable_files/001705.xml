    &lt;ingredients&gt;
        &lt;item&gt;
            &lt;name&gt;Boneless, skinless chicken breasts&lt;/name&gt;
            &lt;quantity&gt;1.5&lt;/quantity&gt;
            &lt;unit&gt;pounds&lt;/unit&gt;
        &lt;/item&gt;
        &lt;item&gt;
            &lt;name&gt;Bell peppers&lt;/name&gt;
            &lt;quantity&gt;2&lt;/quantity&gt;
            &lt;unit&gt;pieces&lt;/unit&gt;
        &lt;/item&gt;
        &lt;item&gt;
            &lt;name&gt;Onions&lt;/name&gt;
            &lt;quantity&gt;1&lt;/quantity&gt;
            &lt;unit&gt;medium&lt;/unit&gt;
        &lt;/item&gt;
        &lt;item&gt;
            &lt;name&gt;Fajita seasoning&lt;/name&gt;
            &lt;quantity&gt;1&lt;/quantity&gt;
            &lt;unit&gt;packet&lt;/unit&gt;
        &lt;/item&gt;
    &lt;/ingredients&gt;
    &lt;steps&gt;
        &lt;step&gt;
            &lt;description&gt;Slice the chicken breasts into thin strips and season with fajita seasoning.&lt;/description&gt;
            &lt;duration&gt;10&lt;/duration&gt;
            &lt;tools&gt;slicing knife, cutting board&lt;/tools&gt;
        &lt;/step&gt;
        &lt;step&gt;
            &lt;description&gt;Heat a large skillet over medium-high heat and add the chicken. Cook for 5-7 minutes or until browned.&lt;/description&gt;
            &lt;duration&gt;7&lt;/duration&gt;
            &lt;tools&gt;skillet, spatula&lt;/tools&gt;
        &lt;/step&gt;
        &lt;step&gt;
            &lt;description&gt;Add the sliced bell peppers and onions to the skillet and cook until tender, about 5 minutes.&lt;/description&gt;
            &lt;duration&gt;5&lt;/duration&gt;
            &lt;tools&gt;skillet, spatula&lt;/tools&gt;
        &lt;/step&gt;
    &lt;/steps&gt;
&lt;/recipe&gt;
