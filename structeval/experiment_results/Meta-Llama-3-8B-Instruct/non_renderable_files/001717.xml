
&lt;recipe&gt;
    &lt;title&gt;Delicious Chicken Fajitas&lt;/title&gt;
    &lt;description&gt;A flavorful and spicy dish made with marinated chicken, bell peppers, and onions.&lt;/description&gt;
    &lt;cuisine&gt;Mexican&lt;/cuisine&gt;
    &lt;servings&gt;4&lt;/servings&gt;
    &lt;prepTime&gt;PT30M&lt;/prepTime&gt;
    &lt;cookTime&gt;PT20M&lt;/cookTime&gt;
    &lt;ingredients&gt;
        &lt;ingredient&gt;
            &lt;name&gt;Chicken breast&lt;/name&gt;
            &lt;quantity&gt;1.5&lt;/quantity&gt;
            &lt;unit&gt;pounds&lt;/unit&gt;
            &lt;optional&gt;false&lt;/optional&gt;
        &lt;/ingredient&gt;
        &lt;ingredient&gt;
            &lt;name&gt;Bell peppers&lt;/name&gt;
            &lt;quantity&gt;2&lt;/quantity&gt;
            &lt;unit&gt;pieces&lt;/unit&gt;
            &lt;optional&gt;false&lt;/optional&gt;
        &lt;/ingredient&gt;
        &lt;ingredient&gt;
            &lt;name&gt;Onions&lt;/name&gt;
            &lt;quantity&gt;1&lt;/quantity&gt;
            &lt;unit&gt;medium&lt;/unit&gt;
            &lt;optional&gt;false&lt;/optional&gt;
        &lt;/ingredient&gt;
    &lt;/ingredients&gt;
    &lt;steps&gt;
        &lt;step&gt;
            &lt;order&gt;1&lt;/order&gt;
            &lt;instruction&gt;Marinate the chicken in a mixture of olive oil, lime juice, and spices for at least 30 minutes.&lt;/instruction&gt;
            &lt;duration&gt;PT30M&lt;/duration&gt;
        &lt;/step&gt;
        &lt;step&gt;
            &lt;order&gt;2&lt;/order&gt;
            &lt;instruction&gt;Heat a large skillet over medium-high heat and cook the chicken for 5-7 minutes per side, or until cooked through.&lt;/instruction&gt;
            &lt;duration&gt;PT10M&lt;/duration&gt;
        &lt;/step&gt;
    &lt;/steps&gt;
    &lt;nutrition&gt;
        &lt;calories&gt;400&lt;/calories&gt;
        &lt;protein&gt;30&lt;/protein&gt;
        &lt;fat&gt;20&lt;/fat&gt;
        &lt;carbohydrates&gt;20&lt;/carbohydrates&gt;
    &lt;/nutrition&gt;
&lt;/recipe&gt;
