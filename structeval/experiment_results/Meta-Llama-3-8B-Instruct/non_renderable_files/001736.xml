
&lt;recipe&gt;
    &lt;title&gt;Delicious Chicken Fajitas&lt;/title&gt;
    &lt;author&gt;
        &lt;name&gt;John Doe&lt;/name&gt;
        &lt;id&gt;12345&lt;/id&gt;
    &lt;/author&gt;
    &lt;ingredients&gt;
        &lt;ingredient&gt;
            &lt;name&gt;Boneless, skinless chicken breasts&lt;/name&gt;
            &lt;quantity&gt;1.5&lt;/quantity&gt;
            &lt;unit&gt;pounds&lt;/unit&gt;
            &lt;optional&gt;false&lt;/optional&gt;
        &lt;/ingredient&gt;
        &lt;ingredient&gt;
            &lt;name&gt;Bell peppers&lt;/name&gt;
            &lt;quantity&gt;2&lt;/quantity&gt;
            &lt;unit&gt;pieces&lt;/unit&gt;
            &lt;optional&gt;false&lt;/optional&gt;
        &lt;/ingredient&gt;
    &lt;/ingredients&gt;
    &lt;instructions&gt;
        &lt;step&gt;
            &lt;description&gt;Slice the chicken breasts into thin strips and season with salt, pepper, and your favorite spices.&lt;/description&gt;
            &lt;duration_minutes&gt;10&lt;/duration_minutes&gt;
        &lt;/step&gt;
        &lt;step&gt;
            &lt;description&gt;Heat a large skillet over medium-high heat and add the chicken and bell peppers. Cook for 5-7 minutes or until the chicken is cooked through and the bell peppers are tender.&lt;/description&gt;
            &lt;duration_minutes&gt;12&lt;/duration_minutes&gt;
        &lt;/step&gt;
    &lt;/instructions&gt;
