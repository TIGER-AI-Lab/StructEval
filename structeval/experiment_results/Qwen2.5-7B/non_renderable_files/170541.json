import json
import xml.etree.ElementTree as ET

xml_data = """
<galaxy>
  <name>Andromeda</name>
  <starSystems>
    <starSystem>
      <name>Alpha Prime</name>
      <planets>
        <planet>
          <name>Zephyria</name>
          <moons>
            <moon>Iolet</moon>
            <moon>Vesper</moon>
          </moons>
        </planet>
        <planet>
          <name>Quorra</name>
          <moons>
            <moon>Nyx</moon>
          </moons>
        </planet>
      </planets>
    </starSystem>
    <starSystem>
      <name>Beta Echo</name>
      <planets>
        <planet>
          <name>Solace</name>
          <moons>
            <moon>Echolet</moon>
          </moons>
        </planet>
      </planets>
    </starSystem>
  </starSystems>
</galaxy>
"""

def xml_to_json(xml_string):
    root = ET.fromstring(xml_string)
    json_data = json.dumps(ET.tostring(root, encoding='unicode'), indent=4)
    return json_data

json_data = xml_to_json(xml_data)
print(json_data)