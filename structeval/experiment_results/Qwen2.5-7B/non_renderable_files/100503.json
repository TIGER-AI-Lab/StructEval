python
import toml
import json

toml_data = """
[planet]
name = "Zyphora"
type = "Gas Giant"

[planet.discovery]
year = 2134
method = "Transit Photometry"

[planet.atmosphere]
composition = ["Hydrogen", "Helium", "Methane"]
pressure = 1200.5

planet.moons = ["Lunara", "Eclipsion", "Vesper"]

[planet.rings]
present = true
materials = ["Ice", "Rock", "Dust"]

[planet.surface.temperature]
mean = -180
extremes = [-210, -130]

planet.surface.features = ["Storm Bands", "Auroras", "Ammonia Clouds"]
"""

# Parse the TOML data
parsed_data = toml.loads(toml_data)

# Convert the parsed data to JSON
json_data = json.dumps(parsed_data, indent=4)

print(json_data)