
import xmltodict
import json

xml_data = """
<galacticZoo>
  <animals>
    <animal>
      <name>Zylox</name>
      <species>Quasar Lynx</species>
      <origin>
        <planet>Xenthra</planet>
        <galaxy>Andromeda</galaxy>
      </origin>
    </animal>
    <animal>
      <name>Blippo</name>
      <species>Stellar Puffin</species>
      <origin>
        <planet>Pluvia</planet>
        <galaxy>Milky Way</galaxy>
      </origin>
    </animal>
  </animals>
  <curator>
    <name>Dr. Nebula</name>
    <species>Human</species>
  </curator>
</galacticZoo>
"""

def xml_to_json(xml_string):
    data_dict = xmltodict.parse(xml_string)
    json_data = json.dumps(data_dict)
    return json_data

json_data = xml_to_json(xml_data)
print(json_data)
