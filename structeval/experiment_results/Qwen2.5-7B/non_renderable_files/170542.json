
import xmltodict
import json

xml_data = """
<spaceStation>
  <name>Odyssey Orbital</name>
  <crew>
    <astronaut>Dr. Mira Patel</astronaut>
    <astronaut>Commander Lee Zhang</astronaut>
  </crew>
  <laboratories>
    <lab>
      <name>Astrobiology Bay</name>
      <equipment>Microscope</equipment>
      <equipment>Gene Sequencer</equipment>
    </lab>
  </laboratories>
</spaceStation>
"""

data_dict = xmltodict.parse(xml_data)
json_data = json.dumps(data_dict)

print(json_data)
