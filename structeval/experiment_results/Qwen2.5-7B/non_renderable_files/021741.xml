import xml.etree.ElementTree as ET

# CSV data as a string
csv_data = """
galaxy.name,galaxy.type,galaxy.distance_lightyears,galaxy.discovery.year,galaxy.discovery.telescope,galaxy.discovery.team[0].name,galaxy.discovery.team[0].role,galaxy.discovery.team[1].name,galaxy.discovery.team[1].role,galaxy.planets[0].name,galaxy.planets[0].type,galaxy.planets[0].moons[0].name,galaxy.planets[0].moons[0].diameter_km,galaxy.planets[1].name,galaxy.planets[1].type,galaxy.planets[1].moons[0].name,galaxy.planets[1].moons[0].diameter_km
Andromeda,Spiral,2537000,1923,Hubble Telescope,Edwin Hubble,Lead Astronomer,Milton Humason,Assistant Astronomer,Planet X,Gas Giant,Luna X,3475,Planet Y,Terrestrial,Phobos Y,22.2
"""

# Remove the header row
header = csv_data.split('
')[0]
csv_data = csv_data.split('
')[1:]

# Create the root element
root = ET.Element("galaxies")

# Process each row of CSV data
for row in csv_data:
    galaxy = ET.SubElement(root, "galaxy")
    
    # Split the row into columns
    columns = row.split(',')
    
    # Add elements for each column
    ET.SubElement(galaxy, "name").text = columns[0]
    ET.SubElement(galaxy, "type").text = columns[1]
    ET.SubElement(galaxy, "distance_lightyears").text = columns[2]
    ET.SubElement(galaxy, "discovery.year").text = columns[3]
    ET.SubElement(galaxy, "discovery.telescope").text = columns[4]
    ET.SubElement(galaxy, "discovery.team[0].name").text = columns[5]
    ET.SubElement(galaxy, "discovery.team[0].role").text = columns[6]
    ET.SubElement(galaxy, "discovery.team[1].name").text = columns[7]
    ET.SubElement(galaxy, "discovery.team[1].role").text = columns[8]
    ET.SubElement(galaxy, "planets[0].name").text = columns[9]
    ET.SubElement(galaxy, "planets[0].type").text = columns[10]
    ET.SubElement(galaxy, "planets[0].moons[0].name").text = columns[11]
    ET.SubElement(galaxy, "planets[0].moons[0].diameter_km").text = columns[12]
    ET.SubElement(galaxy, "planets[1].name").text = columns[13]
    ET.SubElement(galaxy, "planets[1].type").text = columns[14]
    ET.SubElement(galaxy, "planets[1].moons[0].name").text = columns[15]
    ET.SubElement(galaxy, "planets[1].moons[0].diameter_km").text = columns[16]

# Generate the XML string
xml_string = ET.tostring(root, encoding='unicode')

# Output the XML string
print(xml_string)