
import xmltodict
import json

xml_data = """
<galaxy>
  <name>Andromeda</name>
  <star_systems>
    <star_system>
      <system_name>Alpha Prime</system_name>
      <planets>
        <planet>
          <planet_name>Zephyria</planet_name>
          <moons>
            <moon>Luna Minor</moon>
            <moon>Selene</moon>
          </moons>
        </planet>
        <planet>
          <planet_name>Obsidian</planet_name>
          <moons>
            <moon>Umbra</moon>
          </moons>
        </planet>
      </planets>
    </star_system>
    <star_system>
      <system_name>Beta Flux</system_name>
      <planets>
        <planet>
          <planet_name>Crystalis</planet_name>
          <moons>
            <moon>Prism</moon>
            <moon>Spectrum</moon>
          </moons>
        </planet>
      </planets>
    </star_system>
  </star_systems>
</galaxy>
"""

def xml_to_json(xml_string):
    data_dict = xmltodict.parse(xml_string)
    json_data = json.dumps(data_dict)
    return json_data

json_data = xml_to_json(xml_data)
print(json_data)
