python
import toml
import json

# TOML code
toml_code = """
[planet]
name = "Gliese 581g"
discovery_year = 2010
orbital_period_days = 36.562
habitable = true
moons = ["Phobos II", "Deimos II"]

  [planet.atmosphere]
  composition = "CO2, N2, O2"
  pressure = 1.2
  has_clouds = false

  [[planet.missions]]
  name = "ExoProbe-1"
  year = 2025
  success = true
    [[planet.missions.crew]]
    name = "Dr. Mira Lee"
    role = "Commander"
    [[planet.missions.crew]]
    name = "Alex Ren"
    role = "Engineer"

  [[planet.missions]]
  name = "Gliese Explorer"
  year = 2031
  success = false
    [[planet.missions.crew]]
    name = "Sonia Patel"
    role = "Scientist"
"""

# Parse TOML code
toml_data = toml.loads(toml_code)

# Convert TOML data to JSON
json_data = json.dumps(toml_data, indent=2)

# Output JSON code
print(json_data)