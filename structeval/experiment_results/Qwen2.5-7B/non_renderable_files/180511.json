import json
import yaml

# Given YAML code
yaml_code = """
galactic_zoo:
  name: 'Andromeda Biodome'
  sector: 'Zeta-9'
  established: 3045
  director:
    name: 'Zyra Quell'
    species: 'Xelthian'
    years_in_charge: 12
  habitats:
    - type: 'Aquatic Sphere'
      climate: 'Hydrothermal'
      inhabitants:
        - species: 'Lumina Ray'
          population: 120
          diet: 'Planktonic'
    - type: 'Crystal Forest'
      climate: 'Icy Temperate'
      inhabitants:
        - species: 'Shardfox'
          population: 34
          diet: 'Mineral-based'
  open_to_public: true
  visitor_facilities:
    cafeteria: true
    guided_tours: true
    gift_shop: false
"""

# Convert YAML to JSON
json_code = json.dumps(yaml.load(yaml_code, Loader=yaml.FullLoader), indent=2)

print(json_code)
