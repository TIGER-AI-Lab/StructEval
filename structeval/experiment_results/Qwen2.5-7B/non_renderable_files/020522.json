import json

# Given CSV data as a string
csv_data = """
galaxy_name,system.id,system.name,system.star_type,system.planets,system.planets[0].planet_name,system.planets[0].type,system.planets[0].moons,system.planets[0].moons[0].moon_name,system.planets[0].moons[0].diameter_km,system.planets[0].moons[0].has_atmosphere,system.planets[0].moons[0].discovered_by,system.planets[0].moons[0].discovery_year
Andromeda,42,Alpha Centauri,Yellow Dwarf,"[{"planet_name": "Xerion", "type": "Gas Giant", "moons": [{"moon_name": "Lunara", "diameter_km": 3200, "has_atmosphere": true, "discovered_by": "Dr. Vega", "discovery_year": 2345}]}]",Xerion,Gas Giant,"[{"moon_name": "Lunara", "diameter_km": 3200, "has_atmosphere": true, "discovered_by": "Dr. Vega", "discovery_year": 2345}]",Lunara,3200,true,Dr. Vega,2345
"""

# Remove the header row
data = csv_data.split('
')[1:]

# Initialize an empty list to store JSON objects
json_data = []

# Process each row
for row in data:
    # Split the row by comma
    fields = row.split(',')
    
    # Extract the JSON string from the 'system.planets' field
    planet_json_str = fields[4].strip('[]')
    
    # Convert the JSON string to a Python dictionary
    planet_dict = json.loads(planet_json_str)
    
    # Create a dictionary for the current system
    system_dict = {
        'galaxy_name': fields[0],
        'system_id': fields[1],
        'system_name': fields[2],
        'system_star_type': fields[3],
        'system_planets': [planet_dict]
    }
    
    # Append the system dictionary to the list
    json_data.append(system_dict)

# Convert the list of dictionaries to a JSON string
json_output = json.dumps(json_data, indent=4)

# Output the JSON string
print(json_output)