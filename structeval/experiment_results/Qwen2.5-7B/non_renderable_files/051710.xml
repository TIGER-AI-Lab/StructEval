
import json
import xml.etree.ElementTree as ET

json_data = '''
{
  "astronautLog": {
    "date": "2042-07-16",
    "mission": {
      "name": "Europa Explorer",
      "crew": [
        { "name": "Dr. Alina Park", "role": "Commander" },
        { "name": "Lt. Raj Patel", "role": "Engineer" }
      ],
      "events": [
        { "timestamp": "09:00", "description": "Landed on Europa's surface" },
        { "timestamp": "14:30", "description": "Deployed ice sample drone" }
      ]
    }
  }
}
'''

def json_to_xml(json_str):
    data = json.loads(json_str)
    root = ET.Element("root")
    astronaut_log = ET.SubElement(root, "astronautLog")
    date = ET.SubElement(astronaut_log, "date")
    date.text = data["astronautLog"]["date"]
    mission = ET.SubElement(astronaut_log, "mission")
    name = ET.SubElement(mission, "name")
    name.text = data["astronautLog"]["mission"]["name"]
    crew = ET.SubElement(mission, "crew")
    for astronaut in data["astronautLog"]["mission"]["crew"]:
        astronaut_elem = ET.SubElement(crew, "astronaut")
        name_elem = ET.SubElement(astronaut_elem, "name")
        name_elem.text = astronaut["name"]
        role_elem = ET.SubElement(astronaut_elem, "role")
        role_elem.text = astronaut["role"]
    events = ET.SubElement(mission, "events")
    for event in data["astronautLog"]["mission"]["events"]:
        event_elem = ET.SubElement(events, "event")
        timestamp_elem = ET.SubElement(event_elem, "timestamp")
        timestamp_elem.text = event["timestamp"]
        description_elem = ET.SubElement(event_elem, "description")
        description_elem.text = event["description"]
    return ET.tostring(root, encoding="unicode")

xml_output = json_to_xml(json_data)
print(xml_output)
