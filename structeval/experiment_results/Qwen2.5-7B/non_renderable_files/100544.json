python
import toml
import json

# TOML code
toml_code = """
[spaceship]
name = "Stellar Nomad"
model = "XJ-9000"

[spaceship.crew]
captain = "Astra Vega"
first_officer = "Lio Kade"

[[spaceship.crew.engineers]]
name = "Rin Taro"
specialty = "Warp Drive"

[[spaceship.crew.engineers]]
name = "Mira Solis"
specialty = "Life Support"

[spaceship.specs]
length_m = 220
mass_t = 1800
power_source = "Antimatter Reactor"

[spaceship.cargo]

[[spaceship.cargo.bays]]
id = "Alpha"
capacity_t = 300

[[spaceship.cargo.bays]]
id = "Beta"
capacity_t = 200

[[spaceship.missions]]
destination = "Kepler-442b"
year = 2321
success = true

[[spaceship.missions]]
destination = "Tau Ceti e"
year = 2323
success = false
"""

# Convert TOML to JSON
toml_data = toml.loads(toml_code)
json_data = json.dumps(toml_data, indent=4)

# Output JSON code
print(json_data)