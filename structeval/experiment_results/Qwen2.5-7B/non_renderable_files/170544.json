
import xmltodict
import json

xml_data = """
<galaxy>
  <name>Andromeda</name>
  <star_systems>
    <star_system>
      <system_name>Zeta Prime</system_name>
      <planets>
        <planet>
          <planet_name>Obelisk</planet_name>
          <moons>
            <moon>Calliope</moon>
            <moon>Thalassa</moon>
          </moons>
        </planet>
        <planet>
          <planet_name>Helion</planet_name>
          <moons>
            <moon>Nyx</moon>
          </moons>
        </planet>
      </planets>
    </star_system>
    <star_system>
      <system_name>Gamma Echo</system_name>
      <planets>
        <planet>
          <planet_name>Vesper</planet_name>
          <moons>
            <moon>Lira</moon>
          </moons>
        </planet>
      </planets>
    </star_system>
  </star_systems>
</galaxy>
"""

def xml_to_json(xml_string):
    data_dict = xmltodict.parse(xml_string)
    json_data = json.dumps(data_dict)
    return json_data

json_data = xml_to_json(xml_data)
print(json_data)
