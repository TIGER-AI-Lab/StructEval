import toml
import json

# TOML code
toml_code = """
galactic_zoo = { name = "Stellar Menagerie", opening_year = 3045, curator = "Dr. Zynox Quark" }

[galactic_zoo.location]
planet = "Nebulon-5"
sector = "Gamma Quadrant"

[[galactic_zoo.species_list]]
name = "Solarian Panther"
