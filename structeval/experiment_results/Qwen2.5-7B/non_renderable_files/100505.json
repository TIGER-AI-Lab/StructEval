python
import toml
import json

# TOML code
toml_code = """
[space_station]
name = "Aurora Outpost"
operational_status = "Active"
mascot = "Cosmo the Astro-Cat"

[space_station.location]
planet = "Ganymede"

[space_station.location.coordinates]
latitude = 12.4567
longitude = 98.7654

space_station.crew = ["Dr. Vega", "Lt. Orion", "Engineer Nova", "Chef Luna"]

[[space_station.modules]]
name = "Habitat Ring"
purpose = "Living Quarters"
status = "Operational"
equipment = ["Beds", "Gym", "Hydroponics"]

[[space_station.modules]]
name = "Research Lab"
purpose = "Experiments"
status = "Maintenance"
equipment = ["Microscopes", "Spectrometers", "Quantum Computer"]

[[space_station.supply_shipments]]
shipment_id = "ST-2024-001"
contents = ["Oxygen Tanks", "Food Rations", "Spare Parts"]
arrival_date = "2024-04-22"

[[space_station.supply_shipments]]
shipment_id = "ST-2024-002"
contents = ["Medical Supplies", "Seeds", "Water Filters"]
arrival_date = "2024-06-15"
"""

# Convert TOML to JSON
toml_data = toml.loads(toml_code)
json_data = json.dumps(toml_data, indent=4)

# Output JSON code
print(json_data)