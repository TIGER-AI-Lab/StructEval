      <name>Alpha Centauri</name>
      <planets>
        <planet>
          <name>Proxima b</name>
          <type>Terrestrial</type>
        </planet>
        <planet>
          <name>Alpha Centauri Bb</name>
          <type>Gas Giant</type>
        </planet>
      </planets>
    </star_system>
    <star_system>
      <name>Sirius</name>
