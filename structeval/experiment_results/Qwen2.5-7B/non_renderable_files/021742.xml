import csv
import xml.etree.ElementTree as ET

# Define the CSV data as a string
csv_data = """
galaxy_name,discovery.year,discovery.telescope,discovery.observers[0].name,discovery.observers[0].role,discovery.observers[1].name,discovery.observers[1].role,characteristics.type,characteristics.distance_light_years,characteristics.mass_solar,characteristics.constellation,satellites[0].name,satellites[0].type,satellites[0].orbital_period_years,satellites[1].name,satellites[1].type,satellites[1].orbital_period_years
NGC 1300,1995,Hubble Space Telescope,Dr. A. Stellar,Lead Astronomer,Ms. B. Nova,Assistant,Barred Spiral,61000000,1.2e11,Eridanus,NGC 1300-A,Globular Cluster,0.3,NGC 1300-B,Dwarf Galaxy,1.7
Andromeda,1924,Mount Wilson Observatory,Dr. E. Hubble,Discoverer,Mr. C. Cosmos,Photographer,Spiral,2537000,1.23e12,Andromeda,Andromeda I,Dwarf Galaxy,2.5,Andromeda II,Dwarf Galaxy,3.2
"""

# Remove the header row from the CSV data
header = csv_data.split('
')[0]
csv_data = '
'.join(csv_data.split('
')[1:])

# Parse the CSV data
reader = csv.DictReader(csv_data.split('
'), delimiter=',', quotechar='"')

# Create the root element for the XML
root = ET.Element("galaxies")

# Iterate over each row in the CSV data
for row in reader:
    # Create a new element for each galaxy
    galaxy = ET.SubElement(root, "galaxy")
    
    # Set the attributes for the galaxy
    for key, value in row.items():
        ET.SubElement(galaxy, key).text = value

# Generate the XML string
xml_string = ET.tostring(root, encoding='unicode')

# Output the XML string
print(xml_string)