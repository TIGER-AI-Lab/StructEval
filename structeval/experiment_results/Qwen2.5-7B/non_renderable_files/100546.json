python
import toml
import json

# TOML code
toml_code = """
[planet]
name = "Auroria Prime"
type = "Gas Giant"
moons = ["Lunara", "Eclipse", "Shadow"]

[planet.discovery]
year = 2384
method = "Gravitational Microlensing"
telescope = "Vera Rubin Observatory"

[planet.habitability]
has_water = false

[planet.habitability.atmosphere]
composition = ["Hydrogen", "Helium", "Methane"]
pressure = 130.5

[planet.habitability.temperature]
average = -180
seasonal_variation = ["Mild", "Extreme"]

[planet.rings]
exists = true
materials = ["Ice", "Dust", "Rock"]

[[planet.visitors]]
species = "Xel'Naga"
arrival_date = "2412-07-19"
mission = "Research"

[[planet.visitors]]
species = "Terran"
arrival_date = "2413-03-02"
mission = "Colonization"
"""

# Convert TOML to JSON
json_data = toml.loads(toml_code)
json_code = json.dumps(json_data, indent=4)

# Output JSON code
print(json_code)