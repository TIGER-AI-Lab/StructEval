import toml
import json

# TOML code
toml_code = """
planet = {
  name = "Zephyra-X9"
  type = "Gas Giant"
  discovery = { 
    year = 2178
    method = "Transit Photometry"
    telescope = "Orion DeepSpace Array"
  }
  moons = ["Lira", "Pax", "Vento"]
  atmosphere = {
    composition = {
      primary = "Hydrogen"
      secondary = "Helium"
    }
    pressure = 120.5
  }
  rings = true
  visitors = [
    { name = "Dr. Elara Myles", species = "Human", arrival_date = "2191-03-14", mission = "Atmospheric Analysis" },
    { name = "Xylo-Tan", species = "Zyphoid", arrival_date = "2192-07-22", mission = "Cultural Exchange" }
  ]
}
"""

# Convert TOML to JSON
json_data = toml.loads(toml_code)
json_code = json.dumps(json_data, indent=2)

print(json_code)
