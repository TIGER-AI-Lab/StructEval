
<recipe>
    <title>Delicious Chicken Fajitas</title>
    <author>
        <name>John Doe</name>
        <contact>johndoe@example.com</contact>
    </author>
    <metadata>
        <cuisine>Mexican</cuisine>
        <difficulty>Medium</difficulty>
        <lastUpdated>2022-07-25T14:30:00Z</lastUpdated>
    </metadata>
    <nutrition>
        <calories>420</calories>
        <fat>24</fat>
        <protein>30</protein>
        <carbohydrates>30</carbohydrates>
    </nutrition>
    <ingredients>
        <item>
            <name>Chicken Breast</name>
            <quantity>500</quantity>
            <unit>grams</unit>
        </item>
        <item>
            <name>Onion</name>
            <quantity>1</quantity>
            <unit>cup</unit>
        </item>
        <item>
            <name>Bell Pepper</name>
            <quantity>1</quantity>
            <unit>cup</unit>
        </item>
        <item>
            <name>Fajita Seasoning</name>
            <quantity>2</quantity>
            <unit>teaspoons</unit>
        </item>
        <item>
            <name>Flour Tortillas</name>
            <quantity>4</quantity>
            <unit>count</unit>
        </item>
    </ingredients>
    <steps>
        <step>
            <description>Heat a large skillet over medium-high heat.</description>
            <duration>2</duration>
            <tools>Skillet, Stovetop</tools>
        </step>
        <step>
            <description>Add 1 tablespoon of oil to the skillet and swirl to coat.</description>
            <duration>1</duration>
            <tools>Skillet, Oil</tools>
        </step>
        <step>
            <description>Add the chicken to the skillet and cook until browned, about 5 minutes.</description>
            <duration>5</duration>
            <tools>Skillet, Chicken</tools>
        </step>
        <step>
            <description>Add the onion and bell pepper to the skillet and cook until tender, about 5 minutes.</description>
            <duration>5</duration>
            <tools>Skillet, Onion, Bell Pepper</tools>
        </step>
        <step>
            <description>Add the fajita seasoning to the skillet and stir to combine.</description>
            <duration>1</duration>
            <tools>Skillet, Fajita Seasoning</tools>
        </step>
        <step>
            <description Warm the flour tortillas by wrapping them in a damp paper towel and microwaving for 20-30 seconds.</description>
            <duration>1</duration>
            <tools>Microwave, Flour Tortillas</tools>
        </step>
        <step>
            <description Assemble the fajitas by placing the cooked chicken and vegetables onto the warmed tortillas.</description>
            <duration>2</duration>
            <tools>Tortillas, Chicken, Vegetables</tools>
        </step>
    </steps>
</recipe>
