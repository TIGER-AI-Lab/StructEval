        <quantity>500</quantity>
        <unit>grams</unit>
    </ingredients>
    <ingredients>
        <name>Onion</name>
        <quantity>1</quantity>
        <unit>cups</unit>
    </ingredients>
    <ingredients>
        <name>Bell Pepper</name>
        <quantity>1</quantity>
        <unit>cups</unit>
    </ingredients>
